{
  "name": "find-duplicated-property-keys",
  "version": "1.0.8",
  "description": "A package for detecting all duplicated property keys of a JSON string.",
  "bin": "build/cli.js",
  "main": "build/index.js",
  "scripts": {
    "build:release": "yarn dist && yarn publish",
    "test": "jest ./src/ --coverage",
    "dist": "standard src/**/*.js && yarn test && ./node_modules/.bin/babel src --out-dir build",
    "start": "node ./build/cli"
  },
  "keywords": [
    "JSON",
    "duplicated",
    "property",
    "keys",
    "validator"
  ],
  "author": "SebastianG",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "app-root-path": "2.2.1",
    "chalk": "2.4.2",
    "command-line-args": "5.1.1",
    "command-line-usage": "5.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "babel-jest": "24.7.1",
    "babel-plugin-add-module-exports": "1.0.2",
    "coveralls": "3.0.3",
    "jest": "24.7.1",
    "jest-html-reporter": "2.5.0",
    "standard": "12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianG77/find-duplicated-property-keys.git"
  },
  "bugs": "https://github.com/SebastianG77/find-duplicated-property-keys/issues",
  "jest": {
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ]
  }
}
